model ps_auths {
  id                       String                     @unique @db.VarChar(255)
  auth_type                pjsip_auth_type_values_v2?
  nonce_lifetime           Int?
  md5_cred                 String?                    @db.VarChar(40)
  password                 String?                    @db.VarChar(80)
  realm                    String?                    @db.VarChar(255)
  username                 String?                    @db.VarChar(40)
  refresh_token            String?                    @db.VarChar(255)
  oauth_clientid           String?                    @db.VarChar(255)
  oauth_secret             String?                    @db.VarChar(255)
  password_digest          String?                    @db.VarChar(1024)
  supported_algorithms_uas String?                    @db.VarChar(1024)
  supported_algorithms_uac String?                    @db.VarChar(1024)

  @@index([id], map: "ps_auths_id")
}

enum pjsip_auth_type_values_v2 {
  md5
  userpass
  google_oauth
}

